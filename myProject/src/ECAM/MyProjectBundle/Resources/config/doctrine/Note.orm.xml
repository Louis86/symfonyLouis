<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="ECAM\MyProjectBundle\Entity\Note" table="note">
    <indexes>
      <index name="IDX_CFBDFA14BCF5E72D" columns="categorie_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="title" type="string" column="title" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="content" type="string" column="content" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="date" type="datetime" column="date" nullable="false"/>
    <many-to-one field="categorie" target-entity="Categorie" fetch="LAZY">
      <join-columns>
        <join-column name="categorie_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
